<%- include("partials/header")%>
<h1> Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <h5>title</h5>
        <input type="text" class="form-control" name="newTitle">
    </div>
    <div class="form-group">
        <h5>post</h5>
        <textarea name="newContent" class="form-control" cols="40" rows="5"></textarea>
    </div>

    <button type="submit" class="btn btn-primary py-5">Publish</button>
</form>
<%- include("partials/footer")%>